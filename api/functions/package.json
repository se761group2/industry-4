{
  "name": "functions",
  "scripts": {
    "lint": "eslint '*/**/*.{js,ts}'",
    "lint:fix": "eslint '*/**/*.{js,ts}' --fix",
    "build": "tsc && node copy-deps",
    "serve": "npm run build && firebase emulators:start --only functions",
    "shell": "npm run build && firebase functions:shell",
    "start": "node copy-deps.js && tsc && concurrently --kill-others \"firebase emulators:start --only functions\" \"tsc --watch\"",
    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log",
    "generate": "graphql-codegen --config codegen.yml"
  },
  "engines": {
    "node": "10"
  },
  "main": "lib/index.js",
  "dependencies": {
    "@types/moment": "^2.13.0",
    "apollo-server-express": "^2.15.0",
    "copyfiles": "^2.3.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "firebase-admin": "^8.10.0",
    "firebase-functions": "^3.6.1",
    "graphql": "14.6.0",
    "graphql-import-files": "^1.0.6",
    "moment": "^2.27.0",
    "node-fetch": "^2.6.1"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "1.15.4",
    "@graphql-codegen/introspection": "1.15.4",
    "@graphql-codegen/typescript": "1.15.4",
    "@graphql-codegen/typescript-resolvers": "1.15.4",
    "@typescript-eslint/eslint-plugin": "^3.5.0",
    "@typescript-eslint/parser": "^3.5.0",
    "apollo": "^2.28.3",
    "concurrently": "^5.2.0",
    "eslint": "^7.3.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "firebase-functions-test": "^0.2.0",
    "lint-staged": "^10.2.11",
    "prettier": "^2.0.5",
    "typescript": "^3.8.0"
  },
  "private": true
}
